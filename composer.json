{
    "name": "rmcgirr83/posteripinviewtopic",
    "type": "phpbb-extension",
    "description": "This extension allows Moderators and Administrators to view the IP of the poster when viewing a topic.",
    "version": "1.2.1",
    "keywords": [
        "phpbb",
        "extension",
        "tools"
    ],
    "time": "2022-01-02",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Rich McGirr (RMcGirr83)",
            "role": "Extension Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "phpbb/phpbb": ">=3.3",
        "composer/installers": "~1.0"
    },
    "extra": {
        "display-name": "Poster IP in Viewtopic",
        "soft-require": {
            "phpbb/phpbb": ">=3.3"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/poster_ip_in_viewtopic",
            "filename": "version_check",
            "ssl": true
        }
    }
}